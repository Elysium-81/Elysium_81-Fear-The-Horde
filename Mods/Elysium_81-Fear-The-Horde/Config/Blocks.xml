<configs>
	<!-- Remove downgrade path -->
	<remove xpath="/blocks/block/property[@name='DowngradeBlock']"/>
	
	
	<!-- Make normal forged a dev block -->
	<remove xpath="/blocks/block[@name='forge']"/>
	
	<append xpath="/blocks/block[@name='rockResource']">
		<drop event="Harvest" name="resourcelimestone" count="8" prob="0.33" tag="oreWoodHarvest"/>
		<drop event="Harvest" name="resourceCopperFragment" count="8" prob="0.33" tag="oreWoodHarvest"/>
		<drop event="Harvest" name="resourceZincFragment" count="8" prob="0.33" tag="oreWoodHarvest"/>
	</append>
	
